<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:ImagePanel="clr-namespace:MediaViewer.UserControls.ImagePanel" xmlns:TabbedExpander="clr-namespace:MediaViewer.UserControls.TabbedExpander" x:Class="MediaViewer.TestWindow"
        xmlns:mwt="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Classic"
        Title="TestWindow" Height="300" Width="300">
    <Grid>
        <ListBox HorizontalAlignment="Left" Height="100" Margin="26,25,0,0" VerticalAlignment="Top" Width="100" ItemsSource="{Binding Observable}"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="26,143,0,0" VerticalAlignment="Top" Width="75" Command="{Binding AddCommand}"/>
        <Button Content="Clear" HorizontalAlignment="Left" Margin="106,144,0,0" VerticalAlignment="Top" Width="75" Command="{Binding ClearCommand}"/>
    </Grid>
</Window>
