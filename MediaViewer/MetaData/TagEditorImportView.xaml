<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:Progress="clr-namespace:MediaViewer.Progress" x:Class="MediaViewer.MetaData.TagEditorImportView"
        Title="Tag Import Progress" Height="350" Width="520" Icon="/MediaViewer;component/Resources/Icons/tag.ico">
    <Grid>
        <Progress:ProgressControl TotalProgress="{Binding TotalProgress}" TotalProgressMax="{Binding TotalProgressMax}"
                                  ItemProgress="{Binding ItemProgress}" ItemProgressMax="100" 
                                  ItemInfo="{Binding ItemInfo}"
                                  InfoMessages="{Binding InfoMessages}"
                                  CancelCommand="{Binding CancelCommand}" OkCommand="{Binding OkCommand}"/>
    </Grid>
</Window>
